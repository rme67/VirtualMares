@model FoxNetSoft.Plugin.Payments.PayPalPlus.Models.PaymentInfoModel
@using FoxNetSoft.Plugin.Payments.PayPalPlus
@using Nop.Web.Framework.UI
@using Nop.Web.Framework
@{
    Layout = "";
    if (Model.APIMode == PayPalAPIMode.Germany)
    {
    	Html.AddScriptParts("https://www.paypalobjects.com/webstatic/ppplus/ppplus.min.js", true);
    }
    else
    {
    	Html.AddScriptParts("https://www.paypalobjects.com/webstatic/ppplusdcc/ppplusdcc.min.js", true);
    }
}
